{
    "name":        "gmazzap/ajax-template-part",
    "description": "Like get_template_part(), but AJAX powered.",
    "license":     "MIT",
    "type":        "wordpress-plugin",
    "keywords":    [
        "wordpress",
        "wordpress plugin",
        "template",
        "template partials",
        "ajax"
    ],
    "authors":     [
        {
            "name":     "Giuseppe Mazzapica",
            "email":    "giuseppe.mazzapica@gmail.com",
            "role":     "Developer"
        }
    ],
    "support":     {
        "issues": "https://github.com/Giuseppe-Mazzapica/ATP/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/ATP"
    },
    "require":     {
        "php":                 ">=5.4",
        "pimple/pimple":       "~3.0",
        "composer/installers": "~1.0",
        "tedivm/stash":        "~0.14.0"
    },
    "autoload":    {
        "psr-4": {
            "GM\\ATP\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "config":      {
        "optimize-autoloader": true
    }
}
